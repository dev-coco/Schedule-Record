{
    "manifest_version": 3,
    "name": "日程记录",
    "description": "记录每项时间的开始和结束时间。",
    "author": "Raz1ner",
    "version": "1.0.5",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "src/icon.png"
    },
    "background": {
      "service_worker": "js/background.js"
    },
    "icons": {
      "128": "src/icon.png"
    },
    "options_page": "options.html",
    "host_permissions": ["https://script.google.com/*"],
    "permissions": ["storage"]
}
